{
  "name": "web-blokus",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "blokus": "0.0.14",
    "classnames": "^2.2.5",
    "express": "^4.15.2",
    "lodash": "^4.17.4",
    "node-sass": "^4.5.0",
    "npm-run-all": "^4.0.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-icons": "^2.2.3",
    "react-scripts": "0.9.4",
    "socket.io": "^1.7.3",
    "socket.io-client": "^1.7.3"
  },
  "scripts": {
    "build-css": "node-sass src/ -o public/stylesheets",
    "watch-css": "node-sass src/ -o public/stylesheets --watch --recursive",
    "start-server": "node server",
    "start-react": "BROWSER=none react-scripts start",
    "start-dev": "npm-run-all -p watch-css start-react start-server",
    "start": "npm run start-server",
    "build": "npm run build-css && react-scripts build",
    "deploy": "npm run build && git push heroku master",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
